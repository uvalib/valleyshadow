        <body id="d4">
            <div1 type="letter" id="d5">
                

                <opener>
                    <dateline>
                        <date value="1867">1867</date>
                    </dateline>
                    <salute><abbr expan="Direct">Dr</abbr> To J. F. Hoffman</salute>
                </opener>

                <p>Richard Banheart</p>

                <p>1867</p>

                <p>
                    <table>

                        <row>
                            <cell>May</cell>
                            <cell>To 1 pair pants &amp; making twill linen</cell>
                            <cell>$1.50</cell>
                        </row>

                        <row>
                            <cell/>
                            <cell>To 1 <unclear>do</unclear> " " "</cell>
                            <cell> 1.50</cell>
                        </row>

                        <row>
                            <cell/>
                            <cell>To 1 shirt &amp; making 75<hi rend="super">c</hi></cell>
                            <cell>75</cell>
                        </row>

                        <row>
                            <cell/>
                            <cell>To 1 <unclear>do</unclear> " 75<hi rend="super">c</hi></cell>
                            <cell>75</cell>
                        </row>

                        <row>
                            <cell>July</cell>
                            <cell>To 15 days lost @ 18 1/4<hi rend="super">c</hi></cell>
                            <cell>2.00</cell>
                        </row>

                        <row>
                            <cell/>
                            <cell>To 1 Armypistol belt &amp; <orig reg="Scabbard"
                                >Scabbert</orig></cell>
                            <cell>6.00</cell>
                        </row>

                        <row>
                            <cell>
                                <abbr expan="August">Augt</abbr>
                            </cell>
                            <cell>To 8 days lost @ 13 1/4</cell>
                            <cell>1.07 1/4</cell>
                        </row>

                        <row>
                            <cell>
                                <abbr expan="September">Sept</abbr>
                            </cell>
                            <cell>To 2 <unclear>do</unclear> " @ 13 1/4</cell>
                            <cell>26 1/2</cell>
                        </row>

                        <row>
                            <cell/>
                            <cell>To Cash 90<hi rend="super">c</hi> 90</cell>
                        </row>

                        <row>
                            <cell/>
                            <cell>To 1/4 <hi rend="super">lb</hi> powder 20<hi rend="super">c</hi></cell>
                            <cell>20</cell>
                        </row>

                        <row>
                            <cell/>
                            <cell>To Cash 25<hi rend="super">c</hi></cell>
                            <cell>25</cell>
                        </row>

                        <row>
                            <cell>
                                <abbr expan="October">Oct</abbr>
                            </cell>
                            <cell>To dress &amp; trimmers for mother @ G.S. Wards</cell>
                            <cell>3.60</cell>
                        </row>

                        <row>
                            <cell>" "</cell>
                            <cell>To Bill N.L. Blakemore $3.05</cell>
                            <cell>3.50</cell>
                        </row>

                        <row>
                            <cell/>
                            <cell>To powder &amp; shot. N.L. Blakemore</cell>
                            <cell>40</cell>
                        </row>

                        <row>
                            <cell>" "</cell>
                            <cell>To making pants &amp; shirt</cell>
                            <cell>75</cell>
                        </row>

                        <row>
                            <cell/>
                            <cell>To " coat $1,00</cell>
                            <cell>1.00</cell>
                        </row>

                        <row>
                            <cell/>
                            <cell/>
                            <cell>-------</cell>
                        </row>
                        <row>
                            <cell>April 20</cell>
                            <cell>Commence work</cell>
                            <cell>$24.86 1/2</cell>
                        </row>

                        <row>
                            <cell><unclear>Dec</unclear> 3</cell>
                            <cell>Quit work</cell>
                            <cell>
                                <unclear>6.00</unclear>
                            </cell>
                        </row>

                        <row>
                            <cell/>
                            <cell>By <del rend="none" hand="h1"/> balance</cell>
                            <cell>$18.86 1/2</cell>
                        </row>

                    </table>
                </p>


                <closer>
                    <salute/>
                    <signed/>
                </closer>
            </div1>
        </body>
    </text>
