        </tagger>
        <address><city>Chambersburg</city><county>Franklin</county><state>Pennsylvania</state></address>
        <date n="1862-02-11">
            <year>1862</year>
            <month norm="02">February</month>
            <day>11</day>
            <weekday id="TUE">Tuesday</weekday>
